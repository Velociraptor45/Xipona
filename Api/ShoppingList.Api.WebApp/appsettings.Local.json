{
  "KeyVault": {
    "Paths": {
      "Database": "database"
    },
    "MountPoint": "dev-ph-shoppinglist",
    "Uri": ""
  },
  "Database": {
    "Name": "dev-shoppinglist",
    "Address": "",
    "Port": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "https://localhost:5000"
    ]
  },
  "Auth": {
    "Enabled": false
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": ".\\logs\\logs.log",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 1048576,
          "retainedFileCountLimit": 40
        }
      }
    ]
  }
}